{
    "compilerOptions": {
        "baseUrl": ".",
        "strict": true,
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "Bundler",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "types": ["vite/client", "vike-react"],
        "noEmit": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "paths": {
            "@assets/*": ["assets/*"],
            "@components/*": ["components/*"],
            "@lib/*": ["lib/*"],
            "@content/*": ["content/*"],
            "@util/*": ["util/*"]
        }
    },
    "exclude": ["dist"]
}
